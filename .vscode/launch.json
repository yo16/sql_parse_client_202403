{
    // prLaunchTaskでnpm debugを起動
    "version": "0.2.0",
    "configurations": [
        {
            "type": "chrome",
            "request": "launch",
            "name": "React起動",
            "url": "http://localhost:3000",
            "webRoot": "${workspaceFolder}/sql_visualizer/src",
            "preLaunchTask": "npm: debug - sql_visualizer",
            "sourceMaps": true,
            "sourceMapPathOverrides": {
                "webpack:///./*": "${webRoot}/src/*"
            }
        }
    ]
}